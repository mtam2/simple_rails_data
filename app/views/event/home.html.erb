<% if !@event.nil? %>
<table align="center">
    <thead>
        <tr>
            <th>Event ID</th>
            <th>Name</th>
            <th>Location</th>
            <th>Price</th>
            <th>Date</th>
            <th>Attendees List</th>
        </tr>
    </thead>
    <tbody>
        <% @event.each do |event| %>
            <tr>
                <td><%= event.id %></td>
                <td><%= event.name %></td>
                <td><%= event.location %></td>
                <td><%= event.price %></td>
                <td><%= event.date %></td>
                <td><a href="/event/<%= event.id %>">Attendees</a></td>
            </tr>
            
        <% end %>
    </tbody>
</table>
<% end %>